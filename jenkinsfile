pipeline {
    agent any
    tools {
        maven 'MAVEN-3.9.8'
    }
    stages {
        stage('GIT') {
            steps {
                git url: 'https://github.com/shagun72/spring-petclinic.git' ,
                    branch: 'main'

            }
        }
        stage('BUILD') {
            steps {
                sh 'mvn --version'
                sh 'mvn validate'
            }
        }
    }
}